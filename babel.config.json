{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "entry",
                "corejs": 3,
                "targets": ["last 2 versions", "not dead", "> 0.2%", "not ie 11"]
            }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/plugin-proposal-export-default-from",
        "@babel/plugin-transform-object-rest-spread"
    ]
}
